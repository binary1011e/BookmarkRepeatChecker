{
    "manifest_version": 3,
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    
    "permissions": [
    "bookmarks",
    "notifications"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "images/bookmark-28-16.png"
    },
    "icons": {
    "16":"images/bookmark-28-16.png",
    "32": "images/bookmark-28-32.png",
    "48": "images/bookmark-28-48.png",
    "128": "images/bookmark-28-128.png"
  },

    "background": {
    "service_worker": "bookmarkscan.js"
    }
}
{
    "manifest_version": 3,
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    
    "permissions": [
    "bookmarks",
    "notifications"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "image.png"
    },
    "background": {
    "service_worker": "bookmarkscan.js"
    }
}